{
    "name": "tcgplayer location enhance experience",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [ "activeTab" ],
    "description": "An extension to display sellers location.",
    "icons": {
        "16": "icons/nasapod16x16.png",
        "32": "icons/nasapod32x32.png",
        "48": "icons/nasapod48x48.png",
        "128": "icons/nasapod128x128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://www.tcgplayer.com/*"],
          "js": ["content.js"],
          "all_frames": true
        }
      ]
}